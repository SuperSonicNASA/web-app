    <div class="container">
      <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-3 gap-4">
        <div class="rounded bg-theme-light p-8 text-center dark:bg-darkmode-theme-light">
          <h4 class="mb-3">
            <a>Giang Tran</a>
          </h4>
          <p class="mb-4">
            University of Waterloo 1st Year Global Business & Art Student
          </p>
        </div>

        <div class="rounded bg-theme-light p-8 text-center dark:bg-darkmode-theme-light">
          <h4 class="mb-3">
            <a>Grace Xu</a>
          </h4>
          <p class="mb-4">
            University of Waterloo 2nd Year Computer Science Student
          </p>
        </div>

        <div class="rounded bg-theme-light p-8 text-center dark:bg-darkmode-theme-light">
          <h4 class="mb-3">
            <a>Son Nguyen</a>
          </h4>
          <p class="mb-4">
            University of Toronto 2nd Year Computer Science Student
          </p>
        </div>

        <div class="rounded bg-theme-light p-8 text-center dark:bg-darkmode-theme-light">
          <h4 class="mb-3">
            <a>Jimmy Nguyen</a>
