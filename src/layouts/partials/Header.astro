---
import Logo from "@/components/Logo.astro";
import ThemeSwitcher from "@/components/ThemeSwitcher.astro";
import config from "@/config/config.json";
import { IoSearch } from "react-icons/io5/index.js";

---

<header class="header z-30">
  <nav class="navbar container">
    <div class="order-0">
      <Logo />
    </div>
    <input id="nav-toggle" type="checkbox" class="hidden" />
    <label
      for="nav-toggle"
      class="order-3 cursor-pointer flex items-center lg:hidden text-dark dark:text-white lg:order-1"
    ></label>
    <ul
      id="nav-menu"
      class="navbar-nav order-3 hidden w-full pb-6 lg:order-1 lg:flex lg:w-auto lg:space-x-2 lg:pb-0 xl:space-x-8"
    >
        <a href="/">Home</a>
        <a href="/team">Team Members</a>
    </ul>
    <div class="order-1 ml-auto flex items-center md:order-2 lg:ml-0">
      <ThemeSwitcher className="mr-5" />
      <a class="btn btn-outline-primary btn-sm hidden lg:inline-block"
            href="https://github.com/orgs/SuperSonicNASA/repositories">
            
            Repo Link
          </a>
    </div>
  </nav>
</header>;
